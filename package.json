{
  "name": "adriantoine.com",
  "version": "1.0.0",
  "description": "Personal website",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf public/bundle* && rm public/assets.json && rm server.built.js",
    "build": "npm run clean && npm run build-client && npm run build-server",
    "build-client": "NODE_ENV=production node build",
    "build-server": "NODE_ENV=production BABEL_ENV=server babel server.js --out-file server.built.js",
    "dev": "rollup -cw --input src/index.js --output public/bundle.js | BABEL_ENV=server babel-node server.js",
    "start": "node server.built.js",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:adriantoine/adriantoine.com.git"
  },
  "engines": {
    "node": "6.x.x",
    "npm": "3.x.x"
  },
  "author": "Adrien Antoine",
  "license": "ISC",
  "homepage": "https://github.com/adriantoine/adriantoine.com#readme",
  "devDependencies": {
    "autoprefixer": "^6.0.3",
    "babel-cli": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-plugin-external-helpers": "^6.8.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.14.0",
    "chalk": "^1.1.3",
    "classnames": "^2.1.3",
    "cssnano": "^3.0.1",
    "eslint": "^3.4.0",
    "eslint-plugin-react": "^6.2.0",
    "filesize": "^3.3.0",
    "gzip-size": "^3.0.0",
    "lost": "^7.1.0",
    "normalize.css": "^4.2.0",
    "postcss-calc": "^5.0.0",
    "postcss-color-function": "^2.0.0",
    "postcss-css-variables": "^0.5.0",
    "postcss-custom-media": "^5.0.0",
    "postcss-extend": "^1.0.1",
    "postcss-import": "8.0.2",
    "postcss-nested": "^1.0.0",
    "postcss-responsive-type": "^0.4.0",
    "postcss-svg": "^1.0.1",
    "react": "^15.3.1",
    "react-document-title": "^2.0.1",
    "react-dom": "^15.3.1",
    "react-router": "^2.7.0",
    "rollup": "^0.34.13",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-commonjs": "^4.1.0",
    "rollup-plugin-filesize": "^0.4.4",
    "rollup-plugin-node-resolve": "^2.0.0",
    "rollup-plugin-postcss": "^0.2.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-uglify": "^1.0.1",
    "rollup-watch": "^2.5.0",
    "twemoji": "^2.1.5"
  },
  "dependencies": {
    "compression": "^1.6.0",
    "express": "^4.13.3",
    "handlebars": "^4.0.2",
    "morgan": "^1.6.1"
  }
}
